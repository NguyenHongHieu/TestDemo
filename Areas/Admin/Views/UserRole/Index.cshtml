@model demo1.Models.Views.UserModel
@using demo1.CodeLogic.Helper
@{
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}


<section class="wrapper mt-2">
    <h3><i class="fa fa-angle-right"></i>&nbsp;@ViewBag.Title</h3>
    <!-- row -->
    <div class="row mt">
        <div class="col-md-12">
            <div class="content-panel col-md-12 no-padding" id="div-UserRoles">
                @Html.Partial("UserRoles", Model)
            </div>
            <!-- /content-panel -->
        </div>
        <!-- /col-md-12 -->
    </div>
    <!-- /row -->
</section>


@section scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            $(".cusleft-user").addClass("active");
            $(".cusleft-userroles").addClass("active-cus");
        })

    </script>
}


